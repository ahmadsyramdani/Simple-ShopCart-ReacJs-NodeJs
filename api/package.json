  "scripts": {
    "superstart": "cd ../xen-web/yarn start",
    "start": "nodemon --exec node ./bin/www",
    "debug": "node --inspect=127.0.0.1:9222 ./bin/www",
    "dev": "nodemon --watch ./ ./bin/www",
    "seed": "node ./seed/*.js",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --coverage --detectOpenHandles",
    "jslint": "./node_modules/.bin/eslint \"models/**/*.js\" \"routes/**/*.js\" \"helpers/**/*.js\" \"dao/**/*.js\" \"controller/**/*.js\" \"app.js\"",
    "jsfmt": "./node_modules/.bin/eslint \"models/**/*.js\" \"routes/**/*.js\" \"helpers/**/*.js\" \"dao/**/*.js\" \"controller/**/*.js\" \"app.js\" --fix",
    "githubtoken": "git config --global url.\"https://${TOKEN}:@github.com/\".insteadOf \"https://github.com/\"",
    "buildexe": "nexe bin/www -r \"./.sequelizerc\" -r \"./app.js\" -r \"./public/\" -r \"./models/\" -r \"./.env\" -r \"./views/\" -r \"./config/\" --verbose"
  },
  "pre-push": [
    "test",
    "jslint"
  ],
